<h1>Editing Question #<%= @question.id %></h1>


<%= form_tag(question_url(@question), method: 'put') do %>

  <div>
    <%= label_tag :q %><br />
    <%= text_field_tag :q, @question.q %>
  </div>

  <div>
    <%= label_tag :votecount %><br />
    <%= text_field_tag :votecount, @question.votecount %>
  </div>

  
  <div>
    <%= submit_tag "Update Question" %>
  </div>

<% end %>
